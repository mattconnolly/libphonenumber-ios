    <territory countryCode="61">
      <availableFormats>
        <numberFormat pattern="(1[389]\d{2})(\d{2})(\d{2})(\d{2})">
          <leadingDigits>
            1(?:
              [38]00|
              90
            )
          </leadingDigits>
          <format>$1 $2 $3 $4</format>
        </numberFormat>
        <numberFormat pattern="(1[389]\d{2})(\d{6})">
          <leadingDigits>
            1(?:
              [38]00|
              90
            )
          </leadingDigits>
          <format>$1 $2</format>
        </numberFormat>
        <numberFormat pattern="([2378])(\d{8})">
          <leadingDigits>[2378]</leadingDigits>
          <format>$1 $2</format>
        </numberFormat>
      </availableFormats>
    </territory>

